                            android:text="Save"
                            android:textSize="8sp"></Button>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="10dp"
                            android:text="No test"
                            android:textColor="#FFFFFF"
                            android:textSize="12sp"
                            android:visibility="gone"></TextView>

                        <androidx.appcompat.widget.SwitchCompat
                            android:id="@+id/swTestDistantMotor"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="0"
                            android:gravity="center"
                            android:visibility="gone"></androidx.appcompat.widget.SwitchCompat>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Test"
                            android:textColor="#FFFFFF"
                            android:textSize="12sp"
                            android:visibility="gone"></TextView>

                        <EditText
                            android:id="@+id/edtStepCheckDistant"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="right"
                            android:inputType="number"
                            android:text="10"
                            android:textColor="#FFFFFF"
                            android:visibility="gone"></EditText>

                        <androidx.appcompat.widget.SwitchCompat
                            android:id="@+id/swStepCheckDistant"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="0"
                            android:gravity="center"
                            android:visibility="gone"></androidx.appcompat.widget.SwitchCompat>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Step"
                            android:textColor="#FFFFFF"
                            android:textSize="12sp"
                            android:visibility="gone"></TextView>

                    </LinearLayout>
                </LinearLayout>
            </HorizontalScrollView>

            <include
                android:id="@+id/layoutSetupData"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                layout="@layout/setupdata" />
            <include
                android:id="@+id/layoutSetupStep"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                layout="@layout/setup_step" />
        </LinearLayout>
    </ScrollView>

    <include
        android:id="@+id/layoutGetDataSetting"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        layout="@layout/get_setting" />

    <include
        android:id="@+id/layoutSaveDataSetting"
        layout="@layout/save_setting"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone" />

    <include
        android:id="@+id/layoutSetup"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        layout="@layout/setup" />

    <include
        android:id="@+id/layoutSetup2"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        layout="@layout/setup_2" />

    <include
        android:id="@+id/layoutListdevice"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        layout="@layout/list_device" />

    <ProgressBar
        android:id="@+id/proBarLoading"
        android:visibility="invisible"
        android:layout_centerInParent="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>


</RelativeLayout>