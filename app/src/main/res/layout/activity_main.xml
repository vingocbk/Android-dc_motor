        android:layout_height="fill_parent"
        android:background="@mipmap/su30_2"
        android:scaleType="fitXY" />

    <TextView
        android:id="@+id/txtNameApp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="10dp"
        android:text="SETUP LANDING GEAR"
        android:textColor="#111312"
        android:textSize="20sp"
        android:textStyle="bold" />

    <ImageView
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_centerHorizontal="true"
        android:background="@mipmap/ic_logo_avy"
        android:visibility="invisible"/>

    <ImageView
        android:id="@+id/imgSelectBackground"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="10dp"
        android:layout_marginRight="5dp"
        android:layout_toLeftOf="@+id/imgSaveDataSetting"
        android:background="@mipmap/ic_gallery"
        android:visibility="visible" />

